CC=gcc -Wall

static: library.c main.c
	@$(CC) -c library.c
	@ar rcs library.a library.o
	@$(CC) -o main main.c library.a

shared: library.c main.c
	@$(CC) -fPIC -c library.c
	@$(CC) -fPIC -shared -o library.so library.o
	@$(CC) -o main main.c library.so -L. -Wl,-rpath=`pwd`

dynamic: main_dynamic.c shared
	@$(CC) -o main main_dynamic.c -L. -ldl -D DYNAMIC

clean:
	@rm -f *.o *.a *.so main library

run_static: static tests_all clean

run_shared: shared tests_all clean

run_dynamic: dynamic tests_all clean

run_all:
	@echo "------------ Biblioteki statyczne ------------"
	@make run_static
	@echo "------------ Biblioteki wspoldzielone ------------"
	@make run_shared
	@echo "------------ Biblioteki dynamiczne ------------"
	@make run_dynamic

run_static_level: CC=gcc -Wall -O$(LEVEL)
run_static_level: static run_static

run_shared_level: CC=gcc -Wall -O$(LEVEL)
run_shared_level: shared run_shared

run_dynamic_level: CC=gcc -Wall -O$(LEVEL)
run_dynamic_level: dynamic run_dynamic

tests_all: tests_small tests_medium tests_big

